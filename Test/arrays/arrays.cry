def main()
  val = "Crystal"
  val += " " + "Clear"
  x = [2, nil, true, "Hello", 6.10, nil, val, val + val]
  x[1] = [[true, [val, "Hello"]], 3]
  k = "sub"
  x += k
  print(x = x + 3 + "hello")
  input()
  #x[1][1] = false
  print_color("Array X:" , 11) 
  print(x)
  val += "\n"
  print(x)
  print("Size: " + size(x) + "\n")
  input()


  #unrolling an array
  print_color("Unroll X:", 11)

  index = 0
  while(index < size(x))
    print(x[index])
   
    if(x[index] == [[true, ["Crystal Clear", "Hello"]], 3])
      print_color("Crystal!", 13)
    end

    index += 1
  end

  print("")

  y = 10 ... 100.0 / 6.3 + 5
  print_color("Range Y:", 11) 
  print(y)
  print("Size: " + size(y) + "\n")

  #unrolling an array
  print_color("Unroll Y + 10:", 11) 
  
  index = 0
  while(index < size(y))
   print(y[index] + 10)
   index += 1
  end

  return x
end