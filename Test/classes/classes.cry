def main()
  print_color("Class Manipulation:", 11)

  # Making an object.
  x = Cube
  print(x)
  print("")

  # Testing object types
  print_color("Type of object: " + type(x), 11)
  print(x == Cube)
  print(x == Box)
  print(x == [nil, nil, nil])

  # Setting Attributes of an object.
  y = 10
  x.@2d = Box
  x.@2d.@width = y
  x.@2d.@height = y * 2
  x.@depth = y + 11.1

  z = 10
  y = fork(x)
  y.@2d.@width = 100 
  y.@2d.@height = 1.35 
  y.@depth = "depth"

  print_color("\nContents of object x:", 11)

  print(x)
  print(x.@2d.@width)
  print(x.@2d.@height)
  print(x.@depth)

  print_color("\nContents of clone " + type(y) + " y:", 11)

  print(y)
  print(y.@2d.@width)
  print(y.@2d.@height)
  print(y.@depth)
  
  print_color("\nCalling area function for x:", 11)
  print(x.area(4, 5))
  
  print_color("\nCalling area function for x.@2d:", 11)
  return x.@2d.area()
end

class Cube
  # Pull out attributes for easy documentation
  @2d
  @depth

  def info()
	print(type(this))
  end
  
  # Get the area of the cube
  def area(width, height)
    print(this)
    print("In Cube's area function!")
	return @2d.area() * @depth
  end
end

class Box
  # Pull out attributes for easy documentation
  @width
  @height
  
  def info()
	print(type(this))
  end

  # Get the area of the box
  def area()
    print(this)
    print("In Box's area function!")
	return @width * @height
  end
end