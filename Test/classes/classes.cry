def main()
  print_color("Class Manipulation:", 11)
  x = Cube
  print("Type of object: " + type(x))
  print(x)
  print(x == Cube)
  print(x == Box)
  print(x == [nil, nil, nil])
  
  return x
end

class Cube
  # Pull out attributes for easy documentation
  @width
  @height
  @depth

  # Get the area of the cube
  def area()
    return @width * @height * @depth
  end
end

class Box
  # Pull out attributes for easy documentation
  @width
  @height

  # Get the area of the cube
  def area()
    return @width * @height
  end
end